    {
      "name": "启动Chrome",
      "type": "chrome",
      "request": "launch",
       "url": "http://0.0.0.0:8080",
      // "runtimeExecutable": "C:\\Program Files (x86)\\Google\\Chrome\\Application\\chrome.exe", // 改成您的 Chrome 安装路径
      "sourceMaps": true,
      "webRoot": "${workspaceRoot}",
      "userDataDir":".chromeDebug",
      "port":9222
